<plugin>
	<activator class="org.orbisgis.Activator" />
	<workspace class="org.orbisgis.workspace.DefaultWorkspace" />
	<application name="OrbisGIS" version-number="1.2.2"
		version-name="Naoned" organization="IRSTV CNRS-FR-2488" />
	
	<!-- windows -->
	<extension-point id="org.orbisgis.Window" schema="schema/window.xsd" />
	
	<!-- global actions -->
	<extension-point id="org.orbisgis.Action" schema="schema/action.xsd" />
	
	<!-- geocatalog wizards -->
	<extension-point id="org.orbisgis.views.geocatalog.NewResourceWizard"
