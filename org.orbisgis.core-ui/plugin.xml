	<!-- geocatalog actions -->
	<extension-point id="org.orbisgis.views.geocatalog.Action"
		name="Contextual actions for resources" schema="schema/resourceAction.xsd" />
	
	<!-- editor actions -->
	<extension-point id="org.orbisgis.editor.Action"
		schema="schema/editorAction.xsd" />
	
	<!-- map editor tools -->
	<extension-point id="org.orbisgis.editors.map.Tool"
		schema="schema/tool.xsd" />
	
	<!-- layer wizards -->
	<extension-point id="org.orbisgis.editors.map.NewLayerWizard"
