<plugin>
	<activator class="org.orbisgis.Activator"/>
	<workspace class="org.orbisgis.workspace.DefaultSwingWorkspace"/>

	<application infoClass="org.orbisgis.OrbisGISApplicationInfo"/>

	<!-- windows -->
	<extension-point id="org.orbisgis.Window" schema="schema/window.xsd"/>

	<!-- global actions -->
	<extension-point id="org.orbisgis.Action" schema="schema/action.xsd"/>

	<!-- geocatalog wizards -->
	<extension-point id="org.orbisgis.views.geocatalog.NewSourceWizard"
		name="GeoCatalog Wizards" schema="schema/sourceWizard.xsd"/>

	<!-- geocatalog actions -->
	<extension-point id="org.orbisgis.views.geocatalog.Action"
		name="Contextual actions for resources"
		schema="schema/sourceAction.xsd"/>

	<!-- geocatalog filters -->
	<extension-point id="org.orbisgis.views.geocatalog.Filter"
		schema="schema/geocatalogFilter.xsd"/>

	<!-- editor actions -->
	<extension-point id="org.orbisgis.editor.Action"
		schema="schema/editorAction.xsd"/>

	<!-- map editor tools -->
	<extension-point id="org.orbisgis.editors.map.Tool" schema="schema/tool.xsd"/>

	<!-- table editor actions -->
	<extension-point id="org.orbisgis.editors.table.ColumnAction"
		schema="schema/tableColumnAction.xsd"/>
	<extension-point id="org.orbisgis.editors.table.CellAction"
		schema="schema/tableCellAction.xsd"/>

	<!-- layer wizards -->
	<extension-point id="org.orbisgis.editors.map.NewLayerWizard"
		schema="schema/newLayerWizard.xsd"/>

	<!-- panels -->
	<extension-point id="org.orbisgis.View" schema="schema/view.xsd"/>
	<extension-point id="org.orbisgis.EditorView" schema="schema/editorView.xsd"/>
	<extension-point id="org.orbisgis.Editor" schema="schema/editor.xsd"/>

	<!-- toc actions -->
	<extension-point id="org.orbisgis.editorViews.toc.Action"
		schema="schema/layerAction.xsd"/>

	<!-- SQL -->
	<extension-point id="org.orbisgis.views.sqlRepository.SQL"
		schema="schema/sqlRepository.xsd"/>

	<!-- geocognition -->
	<extension-point id="org.orbisgis.views.geocognition.Action"
		schema="schema/geocognitionAction.xsd"/>
	<extension-point id="org.orbisgis.views.geocognition.Filter"
		schema="schema/geocognitionFilter.xsd"/>
	<extension-point id="org.orbisgis.views.geocognition.NewWizard"
		schema="schema/geocognitionWizard.xsd"/>

	<!-- Configuration -->
	<extension-point id="org.orbisgis.Configuration" name="Configurations"
		schema="schema/configuration.xsd"/>

	<!-- Legends -->
	<extension-point id="org.orbisgis.LegendPanel" name="Legend Panels"
		schema="schema/legendPanel.xsd"/>

	<!-- Symbols -->
	<extension-point id="org.orbisgis.SymbolPanel" name="Symbol Panels"
