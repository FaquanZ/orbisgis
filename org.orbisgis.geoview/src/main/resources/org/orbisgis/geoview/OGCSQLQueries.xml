		</menuItem>
		<menuItem label="A H2 table">
			<sqlBlock>
				<comment
					value="Register a H2 table. Usage: select register ('dbType', 'host', port, '[path+]dbName', 'user', 'password', 'tableName', 'dsName');"/>
				<sqlInstr
					value="select register('h2','localhost', '0', '[path+]dbName','sa','','tableName', 'dsName');"/>
			</sqlBlock>
		</menuItem>
		<menuItem label="A PostgreSQL/Postgis table">
			<sqlBlock>
				<comment
					value="Register a PostgreSQL/Postgis table. Usage: select register ('dbType', 'host', port, 'dbName', 'user', 'password', 'tableName', 'dsName');"/>
				<sqlInstr
					value="select register('postgresql','host', 'port', 'dbName','user','password','tableName', 'dsName');"/>
			</sqlBlock>
