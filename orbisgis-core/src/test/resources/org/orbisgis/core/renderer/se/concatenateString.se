<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Style xmlns="http://www.opengis.net/se/2.0/core" xmlns:ns2="http://www.opengis.net/ows/2.0" xmlns:ns3="http://www.opengis.net/fes/2.0" xmlns:ns4="http://www.w3.org/1999/xlink">
	<Rule>
		<PointSymbolizer uom="urn:ogc:def:uom:se::mm">
			<PointTextGraphic>
				<PointLabel>
					<StyledText>
						<Text>
							<Concatenate>
								<FormatNumber fallbackValue="8" decimalPoint=":" groupingSeparator="+">
									<NumericValue>
										12345.6
									</NumericValue>
									<Pattern>###,000.##</Pattern>
								</FormatNumber>
								<Categorize fallbackValue="bonjour !">
									<LookupValue>
										<ns3:ValueReference>Appel</ns3:ValueReference>
									</LookupValue>
									<Value>Bonjour</Value>
									<Threshold>12</Threshold>
									<Value>Bon appétit</Value>
									<Threshold>14</Threshold>
									<Value>Bon après-midi</Value>
									<Threshold>18</Threshold>
									<Value>Bonsoir</Value>
								</Categorize>
							</Concatenate>
						</Text>
					</StyledText>
				</PointLabel>
				<PointPosition>
					<X>-60</X>
					<Y>40</Y>
				</PointPosition>
			</PointTextGraphic>
		</PointSymbolizer>
	</Rule>
</Style>

