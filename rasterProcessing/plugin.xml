<plugin>
	<extension point="org.orbisgis.geoview.toc.LayerAction">
		
		<!-- Menu============================================================== -->
		<menu id="org.orbisgis.geoview.StylesMenu" text="Styles..."/>
		<menu id="org.orbisgis.geoview.StylesMenu.DefaultStyleMenu"
			text="Default Style" parent="org.orbisgis.geoview.StylesMenu"/>
		
		<menu id="org.orbisgis.geoview.ProcessingMenu" text="Processing..."/>
		<menu id="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu"
			text="Terrain Analysis" parent="org.orbisgis.geoview.ProcessingMenu"/>
		<menu id="org.orbisgis.geoview.ProcessingMenu.Math"
			text="Math" parent="org.orbisgis.geoview.ProcessingMenu"/>
			
		<menu id="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.TopographyMenu"
			text="Topography" parent="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu"/>
		<menu id="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			text="Hydrology" parent="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu"/>
		
		<!-- Terrain Analysis============================================================== -->
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.topography.ProcessSlopesInPercent"
			text="Slopes (in %)" group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.TopographyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.topography.ProcessSlopesInPercent"/>
		<!-- Hydrology ============================================================== -->
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessD8Direction"
			text="D8: Grid Direction"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessD8Direction"/>
		
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessD8Accumulation"
			text="D8: Grid Accumulation"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessD8Accumulation"/>
						
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessStrahlerStreamOrder"
			text="D8: Strahler Stream Order"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessStrahlerStreamOrder"/>
		
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessOutlets"
			text="D8: Find outlets"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessOutlets"/>
		
		<action
			id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessWatershedFromOutletIndex"
			text="D8: From outlet to watershed"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessWatershedFromOutletIndex"/>
		
		<action id="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessWatersheds"
			text="D8: All watersheds"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.TerrainAnalysisMenu.HydrologyMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.terrainAnalysis.hydrology.ProcessAllWatersheds"/>
		
		<!-- Math ============================================================== -->
		
		<action id="org.orbisgis.geoview.rasterProcessing.action.math"
			text="Add a value"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.Math"
			class="org.orbisgis.geoview.rasterProcessing.action.math.MathAddValue"/>
			
			<action id="org.orbisgis.geoview.rasterProcessing.action.math"
			text="Subtract a value"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.ProcessingMenu.Math"
			class="org.orbisgis.geoview.rasterProcessing.action.math.MathSubtractValue"/>
			
			
		
		<!-- Default Style ============================================================== -->
		
		<action id="org.orbisgis.geoview.action.defaultStyle"
			text="Default Style"
			group="org.orbisgis.rasterProcessing"
			menuId="org.orbisgis.geoview.StylesMenu.DefaultStyleMenu"
			class="org.orbisgis.geoview.rasterProcessing.action.defaultStyle.RasterDefaultStyle"/>
	
	</extension>
	
	<extension point="org.orbisgis.geoview.Action">
		<toolbar id="org.orbisgis.geoview.RasterProcessing" text="Raster Processing"/>
		<action id="org.orbisgis.geoview.tools.RasterProcessing"
			menuId="org.orbisgis.geoview.Tools" text="GeoRaster Calculator"
			menuGroup="org.orbisgis.geoview.tools.Inmediate"
			icon="/org/orbisgis/geoview/rasterProcessing/calculator.png"
			toolbarId="org.orbisgis.geoview.RasterProcessing"
			class="org.orbisgis.geoview.rasterProcessing.toolbar.ImageCalculator"/>
	</extension>
	
	
	
	
	
</plugin>