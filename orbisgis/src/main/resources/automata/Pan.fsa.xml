<automaton initial-status="Standby" 
	package="org.orbisgis.plugin.view.tools.instances.generated" 
	name="Pan"
	icon="pan.png"
	cursor="pan.png"
	tooltip="pan_tooltip"
	command="pan">

	<node name="Standby" text="pan_standby">
		<transition to="OnePointLeft" code="point"/>
	</node>
	<node name="OnePointLeft" text="pan_onepointleft">
		<transition to="RectangleDone" code="point"/>
	</node>
	<node name="RectangleDone">
		<transition to="Standby" code="init"/>
	</node>
	<node name="Cancel"/>
	<transition to="Cancel" code="esc" label="cancel"/>
</automaton>