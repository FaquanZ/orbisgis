<?xml version="1.0" encoding="ISO-8859-1"?>
<menu label="OGC SQL queries">
	<menu label="Register">
		<menuItem label="A file">
			<className value="org.gdms.sql.customQuery.RegisterCall"/>
		</menuItem>
		<menuItem label="A H2 table">
			<sqlBlock>
				<comment value="Register a H2 table"/>
				<sqlInstr
					value="select register('h2','', '0',
					'path+databaseName','','','tableName', 'name');"/>
			</sqlBlock>
		</menuItem>
		<menuItem label="A PostgreSQL/Postgis table">
			<sqlBlock>
				<comment value="Register a PostgreSQL/Postgis table"/>
				<sqlInstr
					value="select register('postgres','host', 'port',
					'databaseName','user','password','tableName',
					'name');"/>
			</sqlBlock>
		</menuItem>
	</menu>
	
